      <Column Name="M_NAME" Member="Username" Type="System.String" DbType="NVarChar(75)" CanBeNull="true" UpdateCheck="Never" />
      <Column Name="M_PASSWORD" Member="Password" Type="System.String" DbType="NVarChar(65)" CanBeNull="true" UpdateCheck="Never" />
      <Column Name="M_EMAIL" Member="Email" Type="System.String" DbType="NVarChar(50)" CanBeNull="true" UpdateCheck="Never" />
      <Association Name="CalEvent_Member" Member="CalEvent" ThisKey="Author" OtherKey="Author" Type="CalEvent" IsForeignKey="true" />
    </Type>
  </Table>
</Database>